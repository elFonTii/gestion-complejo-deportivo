{
  "name": "gestion-complejo-app",
  "productName": "Gestion Complejo Deportivo",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "start": "set NODE_ENV=develompent && electron src/index.js",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . Futbolito --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/favicon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Gestion Complejo Deportivo\"",
    "package-linux": "electron-packager . Futbolito --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/linux/1024x1024.png --prune=true --out=release-builds"
  },
  "keywords": [],
